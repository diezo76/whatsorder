{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "buildCommand": "cd apps/web && npm install -g pnpm && pnpm install && pnpm build",
  "devCommand": "cd apps/web && pnpm dev",
  "installCommand": "npm install -g pnpm && pnpm install",
  "framework": "nextjs",
  "outputDirectory": "apps/web/.next"
}
